<UserControl x:Class="PharmaMev.USC.USC_ALERTS"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PharmaMev.USC"
             mc:Ignorable="d" 
             xmlns:materialdesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             d:DesignHeight="720" d:DesignWidth="700">
    <Grid Background="White">
     
        <ScrollViewer x:Name="scv" Height="650" Width="700" HorizontalAlignment="Right" Margin="0 0 0 0" VerticalAlignment="Bottom" >

            <StackPanel x:Name="stp" TextBlock.FontFamily="Hacen Saudi Arabia">
                <TextBlock x:Name="txtx" FontFamily="Hacen Saudi Arabia" Height="0"/>
            </StackPanel >
        </ScrollViewer>

    </Grid>
</UserControl>
