<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="PharmaMev.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
        </sectionGroup>
    </configSections>
    <connectionStrings>
        <add name="PharmaMev.Properties.Settings.PharmaMevConnectionString"
            connectionString="Data Source=pc;Initial Catalog=PharmaMev;Integrated Security=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <startup useLegacyV2RuntimeActivationPolicy="true"> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
    </startup>
    <userSettings>
        <PharmaMev.Properties.Settings>
            <setting name="ServerName" serializeAs="String">
                <value>PC</value>
            </setting>
            <setting name="DataBase" serializeAs="String">
                <value>PharmaMev</value>
            </setting>
            <setting name="User" serializeAs="String">
                <value>MYS</value>
            </setting>
            <setting name="PassWord" serializeAs="String">
                <value>123</value>
            </setting>
            <setting name="AutoBackUp" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="Path" serializeAs="String">
                <value>C:\\Users\\user\\Desktop\\Back_Up</value>
            </setting>
            <setting name="Quantity" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="Date" serializeAs="String">
                <value>0</value>
            </setting>
        </PharmaMev.Properties.Settings>
    </userSettings>
</configuration>
